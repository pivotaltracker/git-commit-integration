<p id="notice"><%= notice %></p>

<h1>Listing External Link Commits</h1>

<div class="table-responsive">
  <table class="table">
    <thead>
    <tr>
      <th>External link</th>
      <th>Commit</th>
      <th>External ID</th>
      <th>External URI</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @external_link_commits.each do |external_link_commit| %>
      <tr>
        <td><%= external_link_commit.external_link_id %></td>
        <td><%= external_link_commit.commit_id %></td>
        <td><%= external_link_commit.external_id %></td>
        <td><%= link_to external_link_commit.external_uri, external_link_commit.external_uri %></td>
        <td><%= link_to 'Show', elc_path(external_link_commit) %></td>
        <td><%= link_to 'Edit', edit_elc_path(external_link_commit) %></td>
        <td><%= link_to 'Destroy', elc_path(external_link_commit), method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<br>

<%= link_to 'New External link commit', new_elc_path %>
