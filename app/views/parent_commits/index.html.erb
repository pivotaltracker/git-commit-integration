<p id="notice"><%= notice %></p>

<h1>Listing Parent Commits</h1>

<table>
  <thead>
  <tr>
    <th>Commit</th>
    <th>Sha</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @parent_commits.each do |parent_commit| %>
    <tr>
      <td><%= parent_commit.commit_id %></td>
      <td><%= parent_commit.sha %></td>
      <td><%= link_to 'Show', parent_commit %></td>
      <td><%= link_to 'Edit', edit_parent_commit_path(parent_commit) %></td>
      <td><%= link_to 'Destroy', parent_commit, method: :delete, data: {confirm: 'Are you sure?'} %></td>
    </tr>
  <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Parent commit', new_parent_commit_path %>
