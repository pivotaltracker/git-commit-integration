<p id="notice"><%= notice %></p>

<h1>Listing Deploy Commits</h1>

<div class="table-responsive">
  <table class="table">
    <thead>
    <tr>
      <th>Deploy ID</th>
      <th>Deployed sha</th>
      <th colspan="3"></th>
    </tr>
    </thead>

    <tbody>
    <% @deploy_commits.each do |deploy_commit| %>
      <tr>
        <td><%= deploy_commit.deploy_id %></td>
        <td><%= deploy_commit.sha %></td>
        <td><%= link_to 'Show', deploy_commit %></td>
        <td><%= link_to 'Edit', edit_deploy_commit_path(deploy_commit) %></td>
        <td><%= link_to 'Destroy', deploy_commit, method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>

<br>

<%= link_to 'New Deploy commit', new_deploy_commit_path %>
